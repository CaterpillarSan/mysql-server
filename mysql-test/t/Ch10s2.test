#
# Spartan Storage Step2
#
--disable_warnings
install plugin spartan soname 'ha_spartan.so';
drop table if exists t1;
--enable_warnings

CREATE TABLE t1 (
  col_a int,
  col_b varchar(20),
  col_c int
) ENGINE=SPARTAN;

SELECT * FROM t1;

RENAME TABLE t1 TO t2;

#
# Clean up
#
DROP TABLE t2;
uninstall plugin spartan;

